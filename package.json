    "extract-zip": "^2.0.1",
    "https-proxy-agent": "^7.0.6n",
    "node-fetch": "^3.3.2",
    "nodejs-file-downloader": "^4.13.0",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "commander": "^13.1.0"
  },
  "engines": {
