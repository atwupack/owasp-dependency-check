{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES6",
    "strict": true,
    "maxNodeModuleJsDepth": 0,
    "moduleResolution": "nodenext",
    "module": "NodeNext",
    "noEmitOnError": true,
    "alwaysStrict": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false
  },
  "include": ["src"]
}
